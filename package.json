{
  "name": "RNTest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/RNTest/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "build:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "clean": "cd android && gradlew clean && cd ..",
    "restartADB": "cd android && adb kill-server && adb start-server && cd ..",
    "adb": "cd android && adb reverse tcp:9090 tcp:9090 && cd ..",
    "adbDevTool": "cd android && adb reverse tcp:8097 tcp:8097 && cd ..",
    "jetify": "npx jetify",
    "debug": "npx react-devtools",
    "reset": "react-native start -- --reset",
    "prettier": "prettier --write \"src/**/*.{js,ts,tsx}*\" \"*.js\"",
    "prettier-check": "prettier --list-different \"src/**/*.ts*\" \"*.js\"",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/native": "^5.9.0",
    "@react-navigation/stack": "^5.13.0",
    "axios": "^0.21.1",
    "immutability-helper": "^3.1.1",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "native-base": "^2.15.2",
    "react": "16.13.1",
    "react-hook-form": "^6.14.2",
    "react-native": "0.63.4",
    "react-native-action-button": "^2.8.5",
    "react-native-dotenv": "^2.5.0",
    "react-native-elements": "^3.1.0",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-get-random-values": "^1.5.1",
    "react-native-loading-spinner-overlay": "^2.0.0",
    "react-native-modal-selector": "^2.0.3",
    "react-native-pixel-perfect": "^1.0.2",
    "react-native-reanimated": "^1.13.2",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.15.2",
    "react-native-vector-icons": "^8.0.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-immutable": "^5.0.0",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "styled-components": "^5.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-flow": "^7.12.1",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^6.5.1",
    "flow-bin": "^0.142.0",
    "jest": "^25.1.0",
    "jetifier": "^1.6.6",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-devtools": "^4.10.1",
    "react-test-renderer": "16.13.1",
    "reactotron-react-native": "^5.0.0",
    "reactotron-redux": "^3.1.3",
    "redux-devtools-extension": "^2.13.8"
  },
  "jest": {
    "preset": "react-native"
  }
}
